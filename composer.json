{
    "name": "ocr/core",
    "description": "Optical character recognition library.",
    "type": "library",
    "license": "GPL-3.0-only",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "mockery/mockery": "^1.4",
        "phan/phan": "^5.2",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "sebastian/phpcpd": "^6.0",
        "slevomat/coding-standard": "^7.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.11"
    },
    "autoload": {
        "psr-4": {
            "OCR\\": "sources/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OCR\\Test\\": "tests/"
        }
    }
}
